xterm*iconName:Terminal
xterm*eightBitInput:    false
xterm*cursorBlink:      true
xterm.backarrowKey: false
xterm.ttyModes: erase ^?


!nice fonts 
!xterm*font: -misc-fixed-medium-r-semicondensed-*-13-120-75-75-*-*-iso10646-*
xterm*font: -misc-fixed-medium-r-normal-*-14-*-*-*-*-*-iso10646-*

!meh sucky but works
!xterm*font: -*-terminus-medium-*-*-*-14-*-*-*-*-*-iso10646-*

!ugly true type
!xterm*faceName: Monospace
!xterm*faceName: Liberation Mono:size=9
!:antialias=false

xterm*cursorOffTime:    300
xterm*metaSendsEscape:  true

!xterm*faceSize: 9
xterm*Background:     #181818
xterm*title:    Terminal
xterm*cursorOnTime:     300
xterm*utf8:     1
xterm*Foreground:       #E6E6E6
xterm*geometry: 110x31
xterm*cursorUnderLine:  true

!Xft.antialias: 1
!Xft.autohint: 0
!Xft.dpi: 92
!Xft.hinting: 1
!Xft.hintstyle: hintfull
!Xft.lcdfilter: lcddefault
!Xft.rgba: rgb

xterm*boldMode: true
!xterm*locale: checkfont
xterm*jumpScroll: true
xterm*termName: xterm-256color

